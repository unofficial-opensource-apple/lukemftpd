srcdir	= @srcdir@
VPATH	= @srcdir@
SHELL	= /bin/sh

@SET_MAKE@

SUBDIRS	= libnetbsd src

all:	ftpd

ftpd:	@LIBNETBSD@
	( cd src; ${MAKE} )

libnetbsd.a:
	( cd libnetbsd; ${MAKE} )

install clean:
	@for i in ${SUBDIRS}; do \
	    ( echo "$@ ===> $$i" ; cd $$i ; ${MAKE} $@ ); \
	done

distclean: clean
	@for i in ${SUBDIRS}; do \
	    ( echo "$@ ===> $$i" ; cd $$i ; ${MAKE} $@ ); \
	done
	rm -f Makefile config.cache config.log config.status config.h
