PROG	= tnftpd@EXEEXT@
SUBDIRS	= libnetbsd src

srcdir	= @srcdir@
VPATH	= @srcdir@
SHELL	= /bin/sh

@SET_MAKE@


all:	${PROG}

${PROG}: @LIBNETBSD@
	( cd src && ${MAKE} all )

libnetbsd.a:
	( cd libnetbsd && ${MAKE} all )

install clean:
	@for i in ${SUBDIRS}; do \
	    ( echo "$@ ===> $$i" ; cd $$i && ${MAKE} $@ ); \
	done

distclean: clean
	@for i in ${SUBDIRS}; do \
	    ( echo "$@ ===> $$i" ; cd $$i && ${MAKE} $@ ); \
	done
	rm -f Makefile config.cache config.log config.status config.h

.PHONY: all install clean distclean
